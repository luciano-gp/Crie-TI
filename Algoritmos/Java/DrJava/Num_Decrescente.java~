class Num_Decrescente{
  public static void main(String[] args) {
    
    double a, b, c;
    
    a = Entrada.leiaInt("Digite um valor");
    b = Entrada.leiaInt("Digite um valor");
    c = Entrada.leiaInt("Digite um valor");
    
    if (a>b && a>c && c>b) {
      System.out.println (b);
      System.out.println (c);
      System.out.println (a);
    } else if (b>a && b>c && a>c) {
      System.out.println(c);
      System.out.println(a);
      System.out.println(b);
    } else if (b>a && b>c && c>a) {
      System.out.println(a);
      System.out.println(c);
      System.out.println(b);
    } else if (c>a && c>b && a>b) {
      System.out.println (b);
      System.out.println (a);
      System.out.println (c);
    } else if (c>a && c>b && b>a) {
      System.out.println (a);
      System.out.println (b);
      System.out.println (c);
    }
  }
}